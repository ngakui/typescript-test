{
  "name": "iimtest",
  "version": "1.0.0",
  "description": "TD test automatisation",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "pretest": "rimraf app/**/*.js test/**/*.js",
    "test": "jest --watch",
    "start": "node test/golden-master-text-test.js"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "~7.0.18",
    "jest": "^27.5.1",
    "nyc": "~11.0.3",
    "rimraf": "~2.5.2",
    "source-map-support": "0.5.9",
    "ts-jest": "^27.1.3",
    "ts-node": "~3.1.0",
    "typescript": "~3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
